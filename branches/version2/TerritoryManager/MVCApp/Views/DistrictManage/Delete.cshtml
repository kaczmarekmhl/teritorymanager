@model MVCApp.Models.District

@{
    ViewBag.Title = @Strings.DistrictDelete;
}

<h2>@Strings.DistrictDelete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>District</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Number)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Number)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Name)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.PostCodeFirst)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PostCodeFirst)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.PostCodeLast)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PostCodeLast)
    </div>

</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="@Strings.Delete" /> |
        @Html.ActionLink(Strings.BackToList, "Index")
    </p>
}
