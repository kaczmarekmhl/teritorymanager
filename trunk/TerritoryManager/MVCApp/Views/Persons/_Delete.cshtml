@model IEnumerable<MVCApp.Models.Person>

@{
    ViewBag.Title = "Delete Persons";
}


@using (Html.BeginForm("Delete", "Persons", FormMethod.Post))
{   
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <input type="submit" value="Delete" /> 
    @Html.ActionLink("Map", "Index", "Map", new { personList = Model.ToList() }, null)     
    @Html.Partial("~/Views/Persons/_List.cshtml", Model)              
}
